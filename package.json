  "scripts": {
    "dev": "webpack-dev-server --open",
    "prod": "webpack --config webpack.prod.config.js --progress",
    "test": "jest",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || (exit 0)",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
